              "id": "UNTRUSTED_ACTION_SOURCE",
              "name": "Untrusted Action Source",
              "shortDescription": {
                "text": "Untrusted Action Source"
              },
              "fullDescription": {
                "text": "Action is from an untrusted or unknown publisher"
              },
              "defaultConfiguration": {
                "level": "warning"
              },
              "help": {
                "text": "Verify the action is from a trusted source and consider pinning to a specific SHA",
                "markdown": "Verify the action is from a trusted source and consider pinning to a specific SHA"
              },
              "properties": {
                "category": "SUPPLY_CHAIN",
                "precision": "high",
                "problem.severity": "MEDIUM",
                "severity": "MEDIUM",
                "tags": [
                  "security",
                  "ci-cd",
                  "SUPPLY_CHAIN"
                ]
              }
            },
            {
              "id": "REF_CONFUSION",
              "name": "Git Reference Confusion",
              "shortDescription": {
                "text": "Git Reference Confusion"
              },
              "fullDescription": {
                "text": "Action uses ambiguous git reference that could be subject to confusion attacks"
              },
              "defaultConfiguration": {
                "level": "warning"
              },
              "help": {
                "text": "Pin actions to specific commit SHA instead of branch or tag names",
                "markdown": "Pin actions to specific commit SHA instead of branch or tag names"
              },
              "properties": {
                "category": "SUPPLY_CHAIN",
                "precision": "high",
                "problem.severity": "MEDIUM",
                "severity": "MEDIUM",
                "tags": [
                  "security",
                  "ci-cd",
                  "SUPPLY_CHAIN"
                ]
              }
            },
            {
              "id": "UNPINNED_ACTION",
              "name": "Unpinned GitHub Action",
              "shortDescription": {
                "text": "Unpinned GitHub Action"
              },
              "fullDescription": {
                "text": "GitHub Action is not pinned to a specific SHA commit, which may lead to supply chain attacks"
              },
              "defaultConfiguration": {
                "level": "warning"
              },
              "help": {
                "text": "Pin the action to a full 40-character commit SHA instead of a version tag or branch reference. Example: uses: actions/checkout@a12a3943b4bdde767164f792f33f40b04645d846",
                "markdown": "Pin the action to a full 40-character commit SHA instead of a version tag or branch reference. Example: uses: actions/checkout@a12a3943b4bdde767164f792f33f40b04645d846"
              },
              "properties": {
                "category": "MISCONFIGURATION",
                "precision": "high",
                "problem.severity": "MEDIUM",
                "severity": "MEDIUM",
                "tags": [
                  "security",
                  "ci-cd",
                  "MISCONFIGURATION"
                ]
              }
            },
            {
              "id": "ARTIPACKED_VULNERABILITY",
              "name": "Artifact Packing Vulnerability",
              "shortDescription": {
                "text": "Artifact Packing Vulnerability"
              },
              "fullDescription": {
                "text": "Artifact path pattern may include sensitive files or enable path traversal"
              },
              "defaultConfiguration": {
                "level": "warning"
              },
              "help": {
                "text": "Use specific file paths instead of wildcards for artifact upload",
                "markdown": "Use specific file paths instead of wildcards for artifact upload"
              },
              "properties": {
                "category": "SUPPLY_CHAIN",
                "precision": "high",
                "problem.severity": "MEDIUM",
                "severity": "MEDIUM",
                "tags": [
                  "security",
                  "ci-cd",
                  "SUPPLY_CHAIN"
                ]
              }
            },
            {
              "id": "MATRIX_INJECTION",
              "name": "Matrix Strategy Injection",
              "shortDescription": {
                "text": "Matrix Strategy Injection"
              },
              "fullDescription": {
                "text": "Unquoted matrix variable usage may lead to command injection"
              },
              "defaultConfiguration": {
                "level": "warning"
              },
              "help": {
                "text": "Always quote matrix variables in shell commands: \"${{ matrix.var }}\"",
                "markdown": "Always quote matrix variables in shell commands: \"${{ matrix.var }}\""
              },
              "properties": {
                "category": "INJECTION_ATTACK",
                "precision": "high",
                "problem.severity": "MEDIUM",
                "severity": "MEDIUM",
                "tags": [
                  "security",
                  "ci-cd",
                  "INJECTION_ATTACK"
                ]
              }
            },
            {
              "id": "DANGEROUS_WRITE_OPERATION",
              "name": "Dangerous Write Operation",
              "shortDescription": {
                "text": "Dangerous Write Operation"
              },
              "fullDescription": {
                "text": "Direct writes to $GITHUB_OUTPUT or $GITHUB_ENV can lead to command injection vulnerabilities"
              },
              "defaultConfiguration": {
                "level": "error"
              },
              "help": {
                "text": "Use GitHub's recommended secure methods: echo \"name=value\" \u003e\u003e $GITHUB_OUTPUT or use multiline values with EOF delimiters",
                "markdown": "Use GitHub's recommended secure methods: echo \"name=value\" \u003e\u003e $GITHUB_OUTPUT or use multiline values with EOF delimiters"
              },
              "properties": {
                "category": "INJECTION_ATTACK",
                "precision": "high",
                "problem.severity": "CRITICAL",
                "severity": "CRITICAL",
                "tags": [
                  "security",
                  "ci-cd",
                  "INJECTION_ATTACK"
